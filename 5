
#include "pch.h"
#include <iostream>
using namespace std;

int main()
{
	int n,x,k;
	cin >> n;
	int* m = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> m[i];
	}
	x = m[0];
	for (int i = 1; i < n; i++) {
		if (m[i] <= x) {
			m[i-1] = m[i];
			k = i;
		}
	}
	m[k] = x;
	cout << endl;
	for (int i = 0; i < n; i++) {
		cout << m[i] << endl;
	}
	delete[]m;
}


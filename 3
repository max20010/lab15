
#include "pch.h"
#include <iostream>
using namespace std;

int main()
{
	int n;
	cin >> n;
	int* m = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> m[i];
	}
	int max = m[0], max_x = 0, min = m[0], min_x = 0;
	for (int i = 0; i < n; i++) {
		if (m[i] > max) {
			max = m[i];
			max_x = i;
		}		
		if (m[i] < min) {
			min = m[i];
			min_x = i;
		}
	}

	if (max_x < min_x) {
		for (int i = max_x; i < min_x - 1; i++) {
			m[i + 1] = 0;
		}
	}
	if(min_x<max_x){
		for (int i=min_x; i < max_x-1; i++) {
			m[i + 1] = 0;
		}
	}
	
	for (int i = 0; i < n; i++) {
		cout << m[i] << endl;
	}
	delete[]m;
}

